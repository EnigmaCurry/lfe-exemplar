{erl_opts, [debug_info, {src_dirs, ["test"]}]}.
{lfe_first_files, ["src/exemplar-util.lfe",
                   "src/exemplar.lfe",
                   "src/exemplar-xml.lfe",
                   "src/exemplar-html-util.lfe"]}.
{deps_dir, ["deps"]}.
{eunit_compile_opts, [
   {src_dirs, ["test", "src"]}
  ]}.
{deps, [
   {lfe, ".*", {git, "git://github.com/rvirding/lfe.git", {tag, "v0.9.2"}}},
   {lutil, ".*", {git, "git://github.com/lfex/lutil.git", {tag, "0.7.0"}}},
   {clj, ".*", {git, "git://github.com/lfex/clj.git", {tag, "0.3.0"}}},
   {ltest, ".*", {git, "git://github.com/lfex/ltest.git", {tag, "0.7.0"}}}
  ]}.
{provider_hooks, [{pre, [{compile, {lfe, compile}}]}]}.
{plugins, [
   {'lfe-compile', ".*", {git, "git://github.com/lfe-rebar3/compile.git", {tag, "0.2.0"}}},
   {'lfe-version', ".*", {git, "git://github.com/lfe-rebar3/version.git", {tag, "0.2.0"}}}
  ]}.
